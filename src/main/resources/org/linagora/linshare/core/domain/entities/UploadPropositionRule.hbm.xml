<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<typedef
		class="com.t3c.anchel.core.repository.hibernate.type.GenericEnumType" name="uploadPropositionRuleOperatorType">
		<param name="enumClass">com.t3c.anchel.core.domain.constants.UploadPropositionRuleOperatorType</param>
		<param name="identifierMethod">name</param>
		<param name="valueOfMethod">fromString</param>
	</typedef>
	<typedef
		class="com.t3c.anchel.core.repository.hibernate.type.GenericEnumType" name="uploadPropositionRuleFieldType">
		<param name="enumClass">com.t3c.anchel.core.domain.constants.UploadPropositionRuleFieldType</param>
		<param name="identifierMethod">name</param>
		<param name="valueOfMethod">fromString</param>
	</typedef>
	<class name="com.t3c.anchel.core.domain.entities.UploadPropositionRule" table="upload_proposition_rule" lazy="true">
		<id name="id" column="id" type="long" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="uuid" column="uuid" type="string" length="255" not-null="true" lazy="false"/>
		<property access="field" name="operator" column="operator" type="uploadPropositionRuleOperatorType" length="255" not-null="true" lazy="false" />
		<property access="field" name="field" column="field" type="uploadPropositionRuleFieldType" length="255" not-null="true" lazy="false" />
		<property name="value" column="value" type="string" length="255" not-null="false" lazy="false"/>
		<property name="creationDate" column="creation_date" type="timestamp"
			length="6" not-null="true" lazy="false" />
		<property name="modificationDate" column="modification_date"
			type="timestamp" length="6" not-null="true" lazy="false" />
		<many-to-one name="filter" cascade="lock" column="upload_proposition_filter_id" class="com.t3c.anchel.core.domain.entities.UploadPropositionFilter" not-null="true" lazy="no-proxy"/>
	</class>
</hibernate-mapping>
